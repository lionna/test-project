{
  "application": {
    "pdfContentType": "application/pdf",
    "jsonContentType": "application/json",
    "maxRetries": 5,
    "fileSettings": {
      "directoryPath": "files",
      "bufferSize": 8196,
      "DateFormat": "yyyy-MM-dd-HH-mm-ss",
      "daysInCache": 7,
      "cacheKey": "cacheKey"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
